<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="sv_iconDrawableResource" format="reference"/>
    <attr name="sv_disabledColor" format="color"/>
    <attr name="sv_titleText" format="string" localization="suggested"/>
    <attr name="sv_titleTextColor" format="color"/>
    <attr name="sv_descriptionText" format="string" localization="suggested"/>
    <attr name="sv_descriptionTextColor" format="color"/>
    <attr name="sv_checked" format="boolean"/>

    <declare-styleable name="sv_ButtonSetting">
        <attr name="sv_iconDrawableResource"/>
        <attr name="sv_disabledColor"/>
        <attr name="sv_titleText"/>
        <attr name="sv_titleTextColor"/>
        <attr name="sv_descriptionText"/>
        <attr name="sv_descriptionTextColor"/>
        <attr name="sv_isCheckable" format="boolean"/>
        <attr name="sv_checked"/>
        <attr name="sv_checkmarkDrawableResource" format="reference"/>
    </declare-styleable>

    <declare-styleable name="sv_SwitchSetting">
        <attr name="sv_iconDrawableResource"/>
        <attr name="sv_disabledColor"/>
        <attr name="sv_titleText"/>
        <attr name="sv_titleTextColor"/>
        <attr name="sv_descriptionText"/>
        <attr name="sv_descriptionTextColor"/>
        <attr name="sv_checked"/>
    </declare-styleable>

    <declare-styleable name="sv_RadiogroupSetting">
        <attr name="sv_iconDrawableResource"/>
        <attr name="sv_disabledColor"/>
        <attr name="sv_titleText"/>
        <attr name="sv_titleTextColor"/>
        <attr name="sv_descriptionText"/>
        <attr name="sv_descriptionTextColor"/>
        <attr name="sv_selected" format="integer"/>
        <attr name="sv_radioButtonLabelTextColor" format="color"/>
        <attr name="sv_firstRadioButtonText" format="string" localization="suggested"/>
        <attr name="sv_secondRadioButtonText" format="string" localization="suggested"/>
    </declare-styleable>

    <declare-styleable name="sv_SeekbarSetting">
        <attr name="sv_iconDrawableResource"/>
        <attr name="sv_disabledColor"/>
        <attr name="sv_titleText"/>
        <attr name="sv_titleTextColor"/>
        <attr name="sv_descriptionText"/>
        <attr name="sv_descriptionTextColor"/>
        <attr name="sv_seekbarMax" format="integer"/>
        <attr name="sv_seekbarProgress" format="integer"/>
    </declare-styleable>
</resources>

